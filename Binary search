#include<stdio.h>

int binarySearch(int arr[], int n, int x) {
     int lest = 0, right = n-1;

while(left <= right) {
int mid;
		mid =(left+right)/2;
		if (arr[mid] == x)
		    return mid;
		else if(arr[mid] > x)
			right = mid-1;
	else
			left = mid + 1;
}
  return -1;
}
 int main() {
   int n, x;

scanf("%d", &n);

int arr[n];
for(int i = 0; i < n; i++) {
    scanf("%d",&x);

 int result = binarySearch(arr, n, x);

  if (result != -1)
        printf("Element found at index %d\n", result);
        else 
       printf("Element not found\n");

return 0;
}






















