#include<stdio.h>
void main() {
int i,n,key,pos;
	int arr[100];
	int flag = 0;

	
	scanf("%d",&n);
	for(i=0;i<n;i++){
			scanf("%d",&arr[i]);
	}
	scanf("%d",&key);
	for(i=0; i<100; i++){
		if(key == arr[i]){
			
			flag = 1;
			pos = i;
			break;
			
		}
	if (flag == 1){
      printf("found at position %d\n",pos);
} else {
      printf("%d not found\n",key);
   }
}




